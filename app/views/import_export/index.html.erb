<div class="container">
  <h1>import-export</h1>
  <div class="col-sm-4 col-sm-offset-4">
    <div class="row">
      <h4>Generate PSI expense report for <%= link_to @prev_month, '/psi_expense_report/' + @prev_month_num %></h4>
    </div>
    <div class="row">
    	<h4><%= link_to "PSI CSV import", psi_import_path %></h4>
    </div>
    <div class="row">
    	<%= form_tag({action: :import_airports}, multipart: true) do %>
		    <%= file_field_tag 'json_data', :class => "btn" %>
    	<%= submit_tag "Upload Airports JSON", :class => "btn btn-success" %>
  <% end %>
    </div>
  </div>
</div>